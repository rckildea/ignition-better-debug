{
    "name": "rckildea/ignition-better-debug",
    "description": "Better control over debug messages in Ignition.",
    "keywords": [
        "laravel",
        "flare",
        "ignition"
    ],
    "homepage": "https://github.com/rckildea/ignition-better-debug",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Kildea",
            "email": "rckildea@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facade/ignition": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "rckildea\\BetterDebug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rckildea\\BetterDebug\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rckildea\\BetterDebug\\TabServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
